<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape>
            <!-- 高度40 -->
            <size android:width="20dp" android:height="20dp" />    <!-- 圆角弧度 20 -->
            <corners android:radius="20dp" />            <!-- 变化率 -->
            <gradient android:endColor="#eeeeee" android:startColor="#eeeeee" />
            <stroke android:width="1dp" android:color="#33da33" />

        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <!-- 高度40 -->
            <size android:width="20dp" android:height="20dp" />    <!-- 圆角弧度 20 -->
            <corners android:radius="20dp" />            <!-- 变化率 -->
            <gradient android:endColor="#eeeeee" android:startColor="#eeeeee" />
            <stroke android:width="1dp" android:color="#666666" />
        </shape>
    </item>
</selector>